<%- include('partials/header'); -%>

<a href="/compose"><button type="button" class="btn btn-primary" id="btn-post">Create New Post</button></a>

<h1>Home</h1>

<p><%= homeContent %> </p>

<% posts.forEach(function(post) { %>

<h3><%= post.title %></h3>
<p>
  <%= post.content.substr(0, 100) + " ..." %>
  <a href="/posts/<%= post.title %>">Read More</a>
</p>

<% }); %>



<%- include('partials/footer'); -%>
